{

  "compilerOptions": {
    "target": "es2015",
		"declaration": true,
    "sourceMap": true,
    "module": "commonjs",
    "baseUrl": ".",
    "removeComments": false,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "types": ["node", "restify", "chai"],
    "outDir": "js/"
  },
  "exclude": [
    "logs",
    "node_modules"
  ],
  "include": ["ts/"]
}
